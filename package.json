{
    "name": "respectmyeyes",
    "version": "2.2.13",
    "displayName": "RespectMyEyes theme",
    "description": "A light theme that respects your eyes.",
    "publisher": "dima-iholkin",
    "author": "Dima Iholkin",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/dima-iholkin/RespectMyEyes-VSCode"
    },
    "engines": {
        "vscode": "^1.40.0"
    },
    "scripts": {
        "build": "vsce package",
        "build-and-install": "vsce package --out ./latest.vsix && code --install-extension latest.vsix && del /f latest.vsix"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "VSCode",
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "RespectMyEyes",
                "uiTheme": "vs",
                "path": "./themes/RespectMyEyes-color-theme.json"
            },
            {
                "label": "RespectMyEyes-Next",
                "uiTheme": "vs",
                "path": "./themes/RespectMyEyes-Next-color-theme.json"
            }
        ]
    },
    "devDependencies": {
        "vsce": "^1.76.1"
    },
    "license": "MIT"
}