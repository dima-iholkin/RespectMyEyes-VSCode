{
  "name": "respectmyeyes",
  "version": "7.0.0-beta.1",
  "displayName": "RespectMyEyes theme",
  "description": "A light theme that respects your eyes.",
  "publisher": "dima-iholkin",
  "author": "Dima Iholkin",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/dima-iholkin/RespectMyEyes-VSCode"
  },
  "engines": {
    "vscode": "^1.40.0"
  },
  "scripts": {
    "build": "npm run generate-variations && vsce package",
    "build-and-install": "npm run generate-variations && vsce package --out ./latest.vsix && code --install-extension latest.vsix && del /f latest.vsix",
    "generate-variations": "pwsh ./generate_variations/GenerateVariations.ps1"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "VSCode",
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "RespectMyEyes Light Grapefruit",
        "uiTheme": "vs",
        "path": "./build/RespectMyEyes_Light_Grapefruit-color-theme.json"
      },
      {
        "label": "RespectMyEyes Light Orange",
        "uiTheme": "vs",
        "path": "./themes/RespectMyEyes-Light-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^1.76.1"
  },
  "license": "MIT"
}